{
  "name": "ringlypro-crm-rachel-integration",
  "version": "2.0.0",
  "description": "RinglyPro CRM with Rachel Voice AI Integration - Real-time appointment booking system",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npm install",
    "setup-db": "node scripts/setup-database.js",
    "seed-db": "node scripts/seed-database.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "create-migration": "npx sequelize-cli migration:generate --name",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:appointments": "jest tests/appointments.test.js",
    "test:integration": "jest tests/rachel-integration.test.js",
    "health-check": "node scripts/health-check.js",
    "appointment-stats": "node scripts/appointment-stats.js",
    "sync-models": "node scripts/sync-models.js",
    "validate-env": "node scripts/validate-environment.js"
  },
  "keywords": [
    "ringlypro",
    "crm", 
    "voice-ai",
    "rachel",
    "appointment-booking",
    "twilio",
    "postgresql",
    "nodejs",
    "real-time-scheduling"
  ],
  "author": "RinglyPro Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "twilio": "^4.19.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "ejs": "^3.1.9",
    "moment-timezone": "^0.5.43",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "express-validator": "^7.0.1",
    "sequelize-cli": "^6.6.2",
    "axios": "^1.6.2",
    "lodash": "^4.17.21",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "sequelize-test-helpers": "^1.4.3",
    "@types/jest": "^29.5.8",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ringlypro/crm-rachel-integration.git"
  },
  "bugs": {
    "url": "https://github.com/ringlypro/crm-rachel-integration/issues"
  },
  "homepage": "https://ringlypro.com",
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/config/**",
      "!src/migrations/**"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src/"
    ],
    "ext": "js,json,ejs",
    "ignore": [
      "node_modules/",
      "tests/",
      "scripts/"
    ]
  }
}